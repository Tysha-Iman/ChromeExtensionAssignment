{
    "manifest_version": 2, 
    "name": "Extension Example",
    "description": "Practicing Chrome Extensions",
    "version": "1.0",
    "permissions": ["activeTab",
        "fontSettings"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "allFrames": true,
            "js": ["/js/jquery.js", "/js/content.js"],
            "css": ["/css/style.css"]
       }
     ],
    "action": {
        "default_icon": {
            "32":"/images/CursorPlaceholder.png"
        },
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [ 
        "images/old-paper.png",
        "images/vignette.png",
        "images/*.png",
        "Fonts/treasure-map-deadhand-font/TreasureMapDeadhand-yLA3.ttf",
        "Fonts/treasure-map-font/Treasuremap-Ea1vj.ttf",
        "images/*.jpg" 
     ] 
}
